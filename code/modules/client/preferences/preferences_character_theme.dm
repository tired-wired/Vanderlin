/datum/preferences/proc/send_character_ui_resources(mob/user)
	switch(char_theme)
		if("dusty")
			send_dusty_theme(user)
		if("grimshart")
			send_grimshart_theme(user)
		if("paper")
			send_paper_theme(user)
		if("parchment")
			send_parchment_theme(user)

